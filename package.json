    "clean": "node scripts/clean.js",
    "copy": "node scripts/copy.js",
    "formatter": "tsfmt -r",
    "ngc": "ngc -p ./tsconfig.aot.json",
    "sass": "node scripts/build-sass.js",
    "serve": "webpack-dev-server --port 4000",
    "start": "npm run build && npm run webpack && npm run serve",
    "start-prod": "npm run ngc && npm run webpack-prod && npm run serve",
    "test": "karma start",
    "webpack": "webpack --config webpack.config.js",
    "webpack-aot": "webpack --config config/webpack.aot.config.js",
    "webpack-prod": "webpack --config config/webpack.prod.config.js"
